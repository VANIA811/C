
#include <stdio.h>

int main()
{
int a,b,c;
printf("Введіть число a = ");
scanf("%d",&a);
printf("Введіть число b = ");
scanf("%d",&b);
printf("Введіть число с = ");
scanf("%d",&c);
if(a>b & a>c)
{
    printf("Число а - найбільше. \n");
}
else if(b>a & b>c)
{
    printf("Число b - найбільше. \n");
}
else if(c>b & c>a)
{
    printf("Число c - найбільше. \n");
}
else
{
    printf("N\n");
}
if(a==b & b==c)
{
    printf("Рівносторонній трикутник. \n");
}
else if((a==b & a!=c) || (a==c & a!=b) || (b==c & b!=a))
{
    printf("Рівнобедрений трикутник. \n");
}
else if(a!=b && b!=c && c!=a)
{
    printf("Різносторонній трикутник. \n");
}
else
{
    printf("N\n");
}
if(a+b>c & c+b>a & a+c>b)
{
    printf("Можна побудувати. \n ");
}
else
{
    printf("Не можна побудувати. \n");
}
}
